{% extends 'base.html.twig' %}

{% block title %}Facture{% endblock %}

{% block body %}
    <h1>Facture</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ facture.id }}</td>
            </tr>
            <tr>
                <th>NumFacture</th>
                <td>{{ facture.numFacture }}</td>
            </tr>
            <tr>
                <th>Services</th>
                <td>{{ facture.services }}</td>
            </tr>
            <tr>
                <th>Designation</th>
                <td>{{ facture.designation }}</td>
            </tr>
            <tr>
                <th>ConditionsFact</th>
                <td>{{ facture.conditionsFact }}</td>
            </tr>
            <tr>
                <th>ReadFacture</th>
                <td>{{ facture.readFacture }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ facture.createdAt ? facture.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ facture.updatedAt ? facture.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('webapp_facture_index') }}">back to list</a>

    <a href="{{ path('webapp_facture_edit', {'id': facture.id}) }}">edit</a>

    {{ include('webapp_facture/_delete_form.html.twig') }}
{% endblock %}

<form method="post" action="{{ path('admin_service_delete', {'id': service.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce service ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ service.id) }}">
    <div class="col-12">
        <button type="submit" class="btn btn-primary">Supprimer</button>
    </div>
</form>

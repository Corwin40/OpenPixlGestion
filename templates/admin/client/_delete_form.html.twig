<form method="post" action="{{ path('admin_client_delete', {'id': client.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce client ? ');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ client.id) }}">
    <div class="col-12">
        <button type="submit" class="btn btn-primary">Supprimer</button>
    </div>
</form>
